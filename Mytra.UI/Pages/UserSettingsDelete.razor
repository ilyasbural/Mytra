@page "/usersettingsdelete/{id:guid}"
@inject HttpClient Http

<PageTitle>UserSettings - @Id</PageTitle>
<h3>UserSettings - @Id</h3>

@code
{
    [Parameter]
    public Guid Id { get; set; }
    private ServiceResponse<UserSettingsResponse>? Response { get; set; }

    protected override async Task OnInitializedAsync()
    {
        Response = await Http.DeleteFromJsonAsync<ServiceResponse<UserSettingsResponse>>
        ($"https://localhost:7046/api/usersettings/{Id}");
    }
}