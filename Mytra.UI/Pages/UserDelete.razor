@page "/userdelete/{id:guid}"
@inject HttpClient Http

<PageTitle>User - @Id</PageTitle>
<h3>User - @Id</h3>

@code
{
    [Parameter]
    public Guid Id { get; set; }
    private ServiceResponse<UserResponse>? Response { get; set; }

    protected override async Task OnInitializedAsync()
    {
        Response = await Http.DeleteFromJsonAsync<ServiceResponse<UserResponse>>
        ($"https://localhost:7046/api/user/{Id}");
    }
}